{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "shrijiadf"
		},
		"ls_shrijidb_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_shrijidb'"
		},
		"ls_shrijidb2_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_shrijidb2'"
		},
		"shrijiadls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'shrijiadls'"
		},
		"ls_shrijidb_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "shrijiserver7474.database.windows.net"
		},
		"ls_shrijidb_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "shrijidb1"
		},
		"ls_shrijidb_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "shriji"
		},
		"ls_shrijidb2_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "shrijiserver7474.database.windows.net"
		},
		"ls_shrijidb2_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "shrijidb2"
		},
		"ls_shrijidb2_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "shriji"
		},
		"shrijiadls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://shrijiadls.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "shrijiadf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_shrijidb_password": {
						"value": "[parameters('ls_shrijidb_password')]"
					},
					"ls_shrijidb2_password": {
						"value": "[parameters('ls_shrijidb2_password')]"
					},
					"shrijiadls_accountKey": {
						"value": "[parameters('shrijiadls_accountKey')]"
					},
					"ls_shrijidb_properties_typeProperties_server": {
						"value": "[parameters('ls_shrijidb_properties_typeProperties_server')]"
					},
					"ls_shrijidb_properties_typeProperties_database": {
						"value": "[parameters('ls_shrijidb_properties_typeProperties_database')]"
					},
					"ls_shrijidb_properties_typeProperties_userName": {
						"value": "[parameters('ls_shrijidb_properties_typeProperties_userName')]"
					},
					"ls_shrijidb2_properties_typeProperties_server": {
						"value": "[parameters('ls_shrijidb2_properties_typeProperties_server')]"
					},
					"ls_shrijidb2_properties_typeProperties_database": {
						"value": "[parameters('ls_shrijidb2_properties_typeProperties_database')]"
					},
					"ls_shrijidb2_properties_typeProperties_userName": {
						"value": "[parameters('ls_shrijidb2_properties_typeProperties_userName')]"
					},
					"shrijiadls_properties_typeProperties_url": {
						"value": "[parameters('shrijiadls_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}